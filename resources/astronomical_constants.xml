<?xml version="1.0" encoding="UTF-8"?>
<constants>
    <c>
        <name>Gravitational constant</name>
        <symbol>G</symbol>
        <value unitsystem="SI" order="-11">6.67408</value>
        <unit>m^3 kg^-1 s^-2</unit>
    </c>
    <c>
        <name>Pi</name>
        <symbol>pi</symbol>
        <value unitsystem="SI" order="0">3.1415926535897932</value>
